											<xsd:element name="PartSpend" minOccurs="0">
												<xsd:complexType>
													<xsd:sequence>
														<xsd:element name="Purchase">
															<xsd:complexType>
																<xsd:sequence>
																	<xsd:element name="product_code" />
																	<xsd:element name="value" />
